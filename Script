            for I = 1, #Objects do
                local Object = Objects[I]
                if game.IsA(Object, "Sound") then
                    Object.TimePosition = Object.TimePosition + 0.00000000000001
                end
            end
        end
    )
    AddCommand(
        Speaker,
        "netlag",
        function()
            target = FindPlayer(args[2])
            NetLag(target)
        end
    )

    AddCommand(
        Speaker,
        "rhand",
        function()
            Speaker.Character.RightHand:Destroy()
        end
    )
    AddCommand(
        Speaker,
        "antikill",
        function()
            antikilling = true
            while antikilling do
                if antikilling == false then
                    break
                end
                Speaker.Character.Humanoid:SetStateEnabled("Seated", false)
                Speaker.Character.Humanoid.Sit = true
                game:GetService("RunService").RenderStepped:Wait()
            end
        end
    )

    AddCommand(
        Speaker,
        "ungtools",
        function()
           ungtools()
        end
    )
       AddCommand(
        Speaker,
        "massplay",
        function()
            MassPlay(args[2], Speaker)
        end
    )
        
    AddCommand(
        Speaker,
        "checkgrabber",
        function()
            gtooler(Speaker)
        end
    )
        AddCommand(
        Speaker,
        "hchat",
