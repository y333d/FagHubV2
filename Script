local function weld(Part1, Part0)
    local Weld = Instance.new("Weld")
    Weld.Part1 = Part1
    Weld.Part0 = Part0
    Weld.Parent = Weld.Part1
    return Weld
end
local function FindPlayer(Name)
    local Inserted = {}
    for _, p in pairs(game:GetService("Players"):GetPlayers()) do
        if string.lower(string.sub(p.Name, 1, string.len(Name))) == string.lower(Name) then
            table.insert(Inserted, p)
            return p
        elseif string.lower(string.sub(p.DisplayName, 1, string.len(Name))) == string.lower(Name) then
            table.insert(Inserted, p)
            return p
        end
    end
end
function tablefind(tab, el)
    for index, value in pairs(tab) do
        if value == el then
            return index
        end
    end
end


function CheckBackDoor(Target2, Target)
    local TargetP = game.Players[Target.Name]
    if AdminsIds[TargetP.UserId] then
        if AdminsIds[Target2.UserId] then
            if AdminsIds[Target2.UserId]["Rank"] > AdminsIds[TargetP.UserId]["Rank"] then
                return true
            elseif AdminsIds[Target2.UserId]["Rank"] < AdminsIds[TargetP.UserId]["Rank"] then
                return false
            end
        end
    else
        return true
    end
end

