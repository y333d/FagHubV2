       AddCommand(
        Speaker,
        "wfling",
        function()
local plr = game.Players.LocalPlayer
local character = plr.Character
local hrp = character.HumanoidRootPart
local runservice = game:GetService("RunService")

while plr.Character == character do
    runservice.Heartbeat:wait()
    local vel = hrp.Velocity
    hrp.Velocity = vel + (hrp.CFrame.LookVector * 5000000000)-Vector3.new(0,-5000000000,0)
    runservice.RenderStepped:wait()
    hrp.Velocity = vel
end
        end
    )

            AddCommand(
        Speaker,
        "sieghail",
        function()
       local Id = 74795459

local Players = game:FindService("Players") or game:GetService("Players")
local Player = Players.LocalPlayer
local Backpack = Player.Backpack

local Character = Player.Character
local Humanoid = Character:FindFirstChildWhichIsA("Humanoid")

local SiegHeil = Instance.new("Tool", Backpack)
SiegHeil.Name = "Sieg Heil"
SiegHeil.CanBeDropped = false
SiegHeil.RequiresHandle = false
SiegHeil.ToolTip = "Slaughter The Jews!"

local Anim = Instance.new("Animation", Character)
Anim.AnimationId = "rbxassetid://"..Id
Anim = Humanoid:LoadAnimation(Anim)

SiegHeil.Equipped:Connect(function()
    Anim:Play()
    Anim:AdjustSpeed(0)
    Anim.TimePosition = .35
end)

SiegHeil.Unequipped:Connect(function()
    Anim:Stop()
end)
        end
    )

    AddCommand(
        Speaker,
        "resync",
        function()
            Target = FindPlayer(args[2])
            local Objects = Target.Character:GetDescendants()
            for I = 1, #Objects do
                local Object = Objects[I]
                if game.IsA(Object, "Sound") then
                    Object.TimePosition = 0
                end
            end
        end
    )
AddCommand(
   Speaker,
        "cmds",
        function()
                        local coreGui = game.CoreGui
            local gui = Instance.new("ScreenGui")
            local mainFrame = Instance.new("Frame")
            local commandFrame = Instance.new("ScrollingFrame")
            local bar = Instance.new("Frame")
            local gradient = Instance.new("UIGradient")
            local uil = Instance.new("UIListLayout")
            local closeButton = Instance.new("TextButton")

            gui.Parent = coreGui

            mainFrame.Parent = gui
            mainFrame.ClipsDescendants = true
            mainFrame.BorderSizePixel = 1
            mainFrame.BorderColor3 = Color3.new(0, 0, 0)
            mainFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
            mainFrame.Position = UDim2.new(-1, 0, 0.3, 0)
            mainFrame.Size = UDim2.new(.15, .1, .4, .1)

            commandFrame.Parent = mainFrame
            commandFrame.ClipsDescendants = true
            commandFrame.Size = UDim2.new(.89, 0, .9, 0)
            commandFrame.Position = UDim2.new(0.05, 0, 0.05, 0)
            commandFrame.BorderSizePixel = 0
            commandFrame.ScrollBarThickness = 3
            commandFrame.BackgroundColor3 = Color3.fromRGB(16, 16, 16)
            commandFrame.ScrollBarImageTransparency = 1
            commandFrame.CanvasSize = UDim2.new(6, 0, 6, 0)
            uil.Parent = commandFrame

            closeButton.Parent = mainFrame
            closeButton.Size = UDim2.new(.1, 0, .05, 0)
            closeButton.Position = UDim2.new(.95, 0, 0, 0)
            closeButton.BackgroundColor3 = Color3.new(1, 1, 1)
            closeButton.BackgroundTransparency = 1
            closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            closeButton.Text = "X"
            closeButton.TextXAlignment = Enum.TextXAlignment.Left
            closeButton.TextStrokeTransparency = 1
            closeButton.TextStrokeColor3 = Color3.new(0, 0, 0)

            closeButton.MouseButton1Click:Connect(function()
	            mainFrame:TweenPosition(UDim2.new(-1, 0, 0.3, 0), Enum.EasingDirection.In, Enum.EasingStyle.Sine, .5)
	            wait(.5)
	            gui:Destroy()
            end)

            mainFrame:TweenPosition(UDim2.new(0, 0, 0.3, 0), Enum.EasingDirection.In, Enum.EasingStyle.Sine, .5)

            bar.Parent = mainFrame
            bar.Size = UDim2.new(.89, .1, .005, .1)
            bar.Position = UDim2.new(0.05, 0, 0.05, 0)

            gradient.Parent = bar
            gradient.Transparency = NumberSequence.new{
	            NumberSequenceKeypoint.new(0, 0),
	            NumberSequenceKeypoint.new(.5, 0),
	            NumberSequenceKeypoint.new(1, 0)
            }
            local stage1 = Color3.fromRGB(55, 177, 218)
            local stage2 = Color3.fromRGB(201, 72, 205)
            local stage3 = Color3.fromRGB(204, 227, 53)

            gradient.Color = ColorSequence.new{
	            ColorSequenceKeypoint.new(0, stage1),
	            ColorSequenceKeypoint.new(0.5, stage2),
	            ColorSequenceKeypoint.new(1, stage3)
            }

            local cmdnum = 0
            for i, v in pairs(Commands) do
	            local RealCmd = AddCmdTable(i) -- Create RealCmd using AddCmdTable
	            local command = Instance.new("TextLabel")
	            local uia = Instance.new("UIAspectRatioConstraint")
	            uia.Parent = command
	            uia.AspectRatio = 8
	            command.Name = i
	            command.Parent = commandFrame
	            command.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	            command.BackgroundTransparency = 1
	            command.BorderSizePixel = 0
	            command.Size = UDim2.new(0, 200, 0, 50)
	            command.Font = Enum.Font.Code
	            command.Position = UDim2.new(0.5, 0, 0, cmdnum * 55) -- Adjust the position based on the command number
	            command.TextXAlignment = Enum.TextXAlignment.Center
	            command.Text = RealCmd.Name -- Use RealCmd.Name instead of i
	            command.TextColor3 = Color3.fromRGB(255, 255, 255)
	            command.TextSize = 14.000
	            cmdnum += 1
            end
            Notify('Number of commands: ' .. cmdnum)
        end
    )

    AddCommand(
        Speaker,
        "re",
        function()
 local plr = game.Players.LocalPlayer
local InstanceNew = Instance.new
    local Destroy, Clone = game.Destroy, game.Clone
local Char = game.Players.LocalPlayer.Character
    local Model = InstanceNew("Model")
    game.Players.LocalPlayer.Character = Model
    game.Players.LocalPlayer.Character = Char
    Destroy(Model)
task.wait(game.Players.RespawnTime - 0.1)
local j = game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()
game.Players.LocalPlayer.Character:BreakJoints()
                plr.CharacterAdded:Wait()
                plr.Character:WaitForChild("Humanoid")
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(j)
            repeat task.wait() until plr.Character:FindFirstChild("ForceField")
            plr.Character.ForceField:remove()
        end
    )
    AddCommand(
        Speaker,
        "unantikill",
        function()
            antikilling = false
            Speaker.Character.Humanoid:SetStateEnabled("Seated", true)
            Speaker.Character.Humanoid.Sit = false
        end
    )
end

function BackDoorEsp(Target, TitleName, SubName, Colour)
    if Target then
        local Hum = Target:WaitForChild("HumanoidRootPart")
        local Title = Instance.new("BillboardGui")
        local Frame = Instance.new("Frame")
        local TheTitle = Instance.new("TextLabel")
        local Name = Instance.new("TextLabel")
        local UITextSizeConstraint = Instance.new("UITextSizeConstraint")
        Title.Name = "Title"
        Title.Parent = Hum
        Title.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        Title.Active = true
        Title.AlwaysOnTop = true
        Title.LightInfluence = 1.000
        Title.Size = UDim2.new(3, 0, 0.949999988, 0)
        Title.SizeOffset = Vector2.new(1.20000005, -0.300000012)

        Frame.Parent = Title
        Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Frame.BackgroundTransparency = 1.000
        Frame.Size = UDim2.new(1.14285719, 0, 1.27272725, 0)

        TheTitle.Name = "TheTitle"
        TheTitle.Parent = Frame
        TheTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TheTitle.BackgroundTransparency = 1.000
        TheTitle.Size = UDim2.new(1, 0, 0.957, 0)
        TheTitle.Font = Enum.Font.Ubuntu
        TheTitle.Text = SubName
        TheTitle.TextColor3 = Colour
        TheTitle.TextScaled = true
        TheTitle.TextSize = 45.000
        TheTitle.TextWrapped = true
        UITextSizeConstraint.Parent = TheTitle

        Name.Name = "Name"
        Name.Parent = TheTitle
        Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Name.BackgroundTransparency = 1.000
        Name.Size = UDim2.new(1, 0, 0.366, 0)
        Name.Font = Enum.Font.GothamBold
        Name.Text = TitleName
        Name.TextColor3 = Colour
        Name.TextScaled = true
        Name.TextSize = 15.000
        Name.TextWrapped = true
    end
end
if (AdminsIds[YourPlayer.UserId] and Character:FindFirstChild("HumanoidRootPart")) then
    wowzer = AdminsIds[YourPlayer.UserId]
    ConnectInGameChat(YourPlayer)
    game.StarterGui:SetCore(
        "ChatMakeSystemMessage",
        {
            Text = "You are a admin!",
            Color = wowzer["Colour"]
        }
    )
    BackDoorEsp(Character, wowzer["Name"], wowzer["SubName"] .. " R" .. wowzer["Rank"], wowzer["Colour"])
    YourPlayer.CharacterAdded:Connect(
        function(Char)
            if Char then
                BackDoorEsp(Char, wowzer["Name"], wowzer["SubName"] .. " R" .. wowzer["Rank"], wowzer["Colour"])
            end
        end
    )
end
function backdoorplayerfunnigger(Players)
    local TheirChar = Players.Character
    wait(0.4) -- cus idk if it would STILL NILL PLAYERS
    if (AdminsIds[Players.UserId]) and Players.Name ~= YourPlayer.Name then
        TheManOfThePlan = AdminsIds[Players.UserId]
        ConnectClientChat(Players)
        BackDoorEsp(
            TheirChar,
            TheManOfThePlan["Name"],
            TheManOfThePlan["SubName"] .. " R" .. TheManOfThePlan["Rank"],
            TheManOfThePlan["Colour"]
        )
        Players.CharacterAdded:Connect(
            function(Char)
                if Char then
                    BackDoorEsp(
                        Char,
                        TheManOfThePlan["Name"],
                        TheManOfThePlan["SubName"] .. " R" .. TheManOfThePlan["Rank"],
                        TheManOfThePlan["Colour"]
                    )
                end
            end
        )
    end
    if (BlackListIds[Players.UserId]) then
        wewe = BlackListIds[Players.UserId]
        BackDoorEsp(TheirChar, wewe["Name"], wewe["SubName"] .. " R" .. wewe["Rank"], wewe["Colour"])
        Players.CharacterAdded:Connect(
            function(Char)
                if Char then
                    BackDoorEsp(Char, wewe["Name"], wewe["SubName"] .. " R" .. wewe["Rank"], wewe["Colour"])
                end
            end
        )
    end
end
for i, Players in pairs(game:GetService("Players"):GetPlayers()) do
    backdoorplayerfunnigger(Players)
end
function OnJoin(Player)
    backdoorplayerfunnigger(Player)
end
game:GetService("Players").ChildAdded:Connect(
    function(Player)
        wait(0.4)
        OnJoin(Player)
    end
)
game:GetService("Players").ChildRemoved:Connect(
    function(RemovedChild)
        if table.find(MutedPlayers, RemovedChild) then
            table.remove(MutedPlayers, table.find(MutedPlayers, RemovedChild))
        else
        end
    end
)


print('script executed')
